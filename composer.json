{
  "require": {
    "craftcms/cms": "3.1.8",
    "vlucas/phpdotenv": "^2.4.0",
    "craftcms/element-api": "2.5.4",
    "swixpop/locate": "2.1.0",
    "sebastianlenz/linkfield": "1.0.16",
    "solspace/craft3-freeform": "2.5.11",
    "fortrabbit/craft-copy": "^1.0.0-RC2"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "optimize-autoloader": true
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
