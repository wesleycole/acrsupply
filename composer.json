{
  "require": {
    "craftcms/cms": "^3.1.23",
    "vlucas/phpdotenv": "^2.4.0",
    "craftcms/element-api": "2.6.0",
    "swixpop/locate": "2.1.1",
    "sebastianlenz/linkfield": "1.0.19",
    "fortrabbit/craft-copy": "^1.0.0-RC5",
    "solspace/craft3-calendar": "2.0.24",
    "solspace/craft-freeform": "3.3.1"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "optimize-autoloader": true
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
